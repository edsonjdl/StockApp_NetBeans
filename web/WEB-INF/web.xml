<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Debut</servlet-name>
        <servlet-class>controleur.Debut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RapportActions</servlet-name>
        <servlet-class>controleur.RapportActions</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DebutHist</servlet-name>
        <servlet-class>controleur.DebutHist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DebutComp</servlet-name>
        <servlet-class>controleur.DebutComp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DebutOpp</servlet-name>
        <servlet-class>controleur.DebutOpp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Preparation</servlet-name>
        <servlet-class>controleur.Preparation</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Debut</servlet-name>
        <url-pattern>/Debut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RapportActions</servlet-name>
        <url-pattern>/RapportActions</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DebutHist</servlet-name>
        <url-pattern>/DebutHist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DebutComp</servlet-name>
        <url-pattern>/DebutComp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DebutOpp</servlet-name>
        <url-pattern>/DebutOpp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Preparation</servlet-name>
        <url-pattern>/Preparation</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>accueil.jsp</welcome-file>
    </welcome-file-list>
</web-app>
